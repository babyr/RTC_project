<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
             "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="RTC_project.mapper.BoardImageMapper">
	<insert id="insert">
		INSERT INTO BoardImage (boardId, imageId)
		VALUES (#{boardId}, #{imageId})
	</insert>
	<delete id="deleteByBoardId">
		DELETE BoardImage WHERE boardId = #{boardId}
	</delete>
</mapper>